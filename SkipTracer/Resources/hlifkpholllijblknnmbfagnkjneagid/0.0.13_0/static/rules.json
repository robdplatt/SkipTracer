[
  {
    "id": 1,
    "priority": 1,
    "action": {
      "type": "redirect",
      "redirect": {
        "transform": {
          "queryTransform": {
            "addOrReplaceParams": [
              {
                "key": "hl",
                "value": "en-US"
              }
            ]
          }
        }
      }
    },
    "condition": {
      "regexFilter": "^https://[^\\.]*\\.(google\\.com|recaptcha\\.net|hcaptcha\\.com|hcaptcha\\.net)/(recaptcha|captcha)",
      "resourceTypes": ["sub_frame"]
    }
  }
]
